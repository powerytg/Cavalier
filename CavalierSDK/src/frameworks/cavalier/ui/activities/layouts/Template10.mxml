<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:supportClasses="frameworks.cavalier.ui.activities.supportClasses.*" 
		 implements="frameworks.cavalier.ui.activities.layouts.ILayoutTemplate" >
	<fx:Script>
		<![CDATA[
			
			public function get holders():Array
			{
				return [holder1, holder2, holder3, holder4];
			}
			
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<s:HGroup left="0" right="0" top="0" bottom="0"
			  gap="15">
		<supportClasses:UIPartHolder id="holder1" width="60%" height="100%"/>
		<s:VGroup width="40%" height="100%" gap="15">
			<supportClasses:UIPartHolder id="holder2" width="100%" height="100%"/>
			<supportClasses:UIPartHolder id="holder3" width="100%" height="100%"/>
			<supportClasses:UIPartHolder id="holder4" width="100%" height="100%"/>
		</s:VGroup>
	</s:HGroup> 
	
</s:Group>
